{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://mentriq-technologies.onrender.com/api/$1"
        },
        {
            "source": "/uploads/(.*)",
            "destination": "https://mentriq-technologies.onrender.com/uploads/$1"
        },
        {
            "source": "/(assets|images)/(.*)",
            "destination": "/$1/$2"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}